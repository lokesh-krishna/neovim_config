  "bibliography": {
    "body": "bibliography: references.bib",
    "description": "add bibliography yaml",
    "prefix": "bib"
  },
  "diary header": {
    "body": [
      "---",
      "title: \"$1\"",
      "date: \"$2\"",
      "categories: [$3]",
      "---"
    ],
    "description": "Add minimal yaml header",
    "prefix": "diary"
  },
  "yaml header": {
    "body": [
      "---",
      "title: $1",
      "author: Lokesh Krishna",
      "date: $2",
      "format: html",
      "---"
    ],
    "description": "Add minimal yaml header",
    "prefix": "yml"
  },
  "yaml header revealjs": {
    "body": [
      "---",
      "title: $1",
      "subtitle: $2",
      "author: Lokesh Krishna",
      "institute: Department of Community Medicine, Seth GS Medical College & KEM Hospital",
      "format:",
      "  revealjs:",
      "    incremental: true",
      "    callout-appearance: simple",
      "    transition: slide",
      "    embed-resources: true",
      "    theme: custom.scss",
      "---"
    ],
    "description": "Add yaml header with options for revealjs",
    "prefix": "yml_revealjs"
  },
  "yaml header article": {
    "body": [
      "---",
      "title: $1",
      "subtitle: $2",
      "author: Lokesh Krishna",
      "institute: Department of Community Medicine, Seth GS Medical College & KEM Hospital",
      "format:",
      "  pdf:",
      "    documentclass: scrartcl",
      "    toc: false",
      "    number-sections: false",
      "    mainfont: STIX Two Text",
      "    mathfont: STIX Two Math",
      "    execute:",
      "     echo: false",
      "     warning: false",
      "    include-in-header:",
      "      text: |",
      "        \\addtokomafont{disposition}{\\rmfamily}",
      "        \\addtokomafont{dictum}{\\rmfamily}",
      "        \\addtokomafont{descriptionlabel}{\\rmfamily}",
      "---"
    ],
    "description": "Add yaml header with options for koma script article class",
    "prefix": "yml_article"
  },
  "yaml header with execute options": {
    "body": [
      "---",
      "title: $1",
      "author: Lokesh Krishna",
      "date: $2",
      "format: html",
      "execute:",
      "  echo: true",
      "  warning: false",
      "  message: false",
      "---"
    ],
    "description": "Add yaml header with execution options",
    "prefix": "yml_execute"
  },
  "ref a wrap figure": {
    "body": "Fig. \\ref{fig-$1}",
    "description": "refer to a label for a wrap figure",
    "prefix": "@wrap"
  },
  "native pagebreak": {
    "body": "{{< pagebreak >}}",
    "description": "insert a native pagebreak into the document",
    "prefix": "pagebreak"
  },
  "contents": {
    "body": "{{< contents $1 >}}",
    "description": "rearrange content in documents",
    "prefix": "contents"
  },
  "keyboard shortcuts": {
    "body": "{{ < kbd $1 >}}",
    "description": "describe keyboard shortcuts in documentation",
    "prefix": "kbd"
  },
  "placeholder text": {
    "body": "{{< lipsum >}}",
    "description": "insert standard placeholder text",
    "prefix": "lipsum"
  },
  "raw typst": {
    "body": [
      "```{=typst}",
      "$1",
      "```"
    ],
    "description": "raw typst markup",
    "prefix": "typst_raw"
  },
  "code-chunk": {
    "prefix": [
      "rcc"
    ],
    "description": "insert r code chunk with label",
    "body": [
      "```{r}",
      "#| label: $1",
      "```"
    ]
  },
  "table": {
    "prefix": [
      "tbl"
    ],
    "description": "insert r code chunk with label and table caption",
    "body": [
      "```{r}",
      "#| label: tbl-$1",
      "#| tbl-cap: $2",
      "```"
    ]
  },
  "libraries": {
    "prefix": [
      "libraries"
    ],
    "description": "insert commonly used libraries",
    "body": [
      "# Libraries",
      "library(tidyverse)",
      "library(here)",
      "library(hrbrthemes)",
      "library(janitor)",
      "library(readxl)"
    ]
  },
  "function": {
    "prefix": [
      "function"
    ],
    "description": "insert function template",
    "body": [
      "${1:name} <- function(${2:variable}) {",
      "  ${0}",
      "}"
    ]
  },
  "for": {
    "prefix": [
      "for"
    ],
    "description": "insert a for loop",
    "body": [
      "for (i in ${1}) {",
      "  ${0}",
      "}"
    ]
  }
}
