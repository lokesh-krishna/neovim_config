  "crossref": {
    "prefix": [
      "crossref"
    ],
    "body": [
      "crossref:",
      "  fig-title: Figure",
      "  tbl-title: Table",
      "  title-delim: .",
      "  fig-prefix: Figure",
      "  tbl-prefix: Table",
      "  eq-prefix: Eq."
