{
    "name": "snips",
    "engines": {
        "vscode": "^1.11.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": "quarto",
                "path": "./snippets/quarto.json"
            },
            {
                "language": "markdown",
                "path": "./snippets/markdown.json"
            },
            {
                "language": [
                    "json",
                    "jsonc"
                ],
                "path": "./snippets/json.json"
            },
            {
                "language": [
                    "css",
                    "scss"
                ],
                "path": "./snippets/css.json"
            },
            {
                "language": "tex",
                "path": "./snippets/tex.json"
            },
            {
                "language": "r",
                "path": "./snippets/R.json"
            },
            {
                "language": "desktop",
                "path": "./snippets/desktop.json"
            }
        ]
    }
}
